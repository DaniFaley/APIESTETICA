{
"compilerOptions": {
  "target": "es2016",
  "module": "commonjs",
  "lib": ["es2020"],
  "outDir": "./build",
  "rootDir": "./src",
  "esModuleInterop": true,
  "moduleResolution": "node",
  "strict": true,
  "noUnusedLocals": true,
  "noUnusedParameters": true,
  "noImplicitReturns": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true
}
}